#include <iostream>
#include <complex>
#include <cstdlib>
#include <math.h>

using namespace std;
double fabs(complex<double> x){
	double return_data;
	double r = x.real(), i = x.imag();

	return_data = sqrt( pow(r,2) + pow(i,2) );

	return return_data;
}

